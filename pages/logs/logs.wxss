.time{
  width: 100%;
  height: 80rpx;
  background-color: #EED8AE;
  text-align: center;
  line-height: 80rpx;
  font-size: 13px;
  color: #FFF;
}
.profit{
  width: 100%;
  height: 130rpx;
  background-color: #EED8AE;
  text-align: center;
}
.profit-one{
  font-size: 15px;
  color:#FFFFFF;
}
.profit-two{
  font-size: 12px;
  color:#FFFFFF;
}
.expenditure{
  width: 100%;
  height: 100rpx;
  background-color: #EED8AE;
}
.expenditure-a{
  width: 50%;
  height: 100rpx;
  text-align: center;
}
.section-one{
  width: 750rpx;
  height: 180rpx;
  background-color: #F2F2F2;
  display: table-cell; 
  vertical-align: middle;
  text-align: center;
}
.section-one image{
  width: 700rpx;
  height: 135rpx;
  border-radius: 10rpx;
}
.section-two,.section-four{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}
.flex-view-item-text image{
  width: 10rpx;
  height: 40rpx;  
  vertical-align: middle;
  margin-left: 20rpx;
}
.flex-view-item-text text{
  font-size: 15px;
  margin-left: 10rpx;
}
.section-three{
  width: 100%;
  height: 240rpx;
  background-color: #FFFFFF;
}
.section-three-content{
  width: 100%;
  height: 120rpx;
  display: flex;
}
.section-content{
  width: 50%;
  height: 120rpx;
}
.section-content image{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;

}
.section-content text{
  font-size: 13px;
  vertical-align: center;
  margin-left: 10rpx;
  color: #969696;
}
.section-wealth{
  width: 190rpx;
  height: 60rpx;
  margin: 30rpx auto;
}
.section-five{
  width: 750rpx;
  height: 120rpx;
  background-color: #FFFFFF;
}
.section-five image{
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
}
.section-five text{
  font-size: 13px;
  margin-left: 10rpx;
  color: #969696;
}
.section-five-a{
  width: 700rpx;
  height: 60rpx;
  margin: 0rpx auto;
  position: relative;
  top:30rpx;
}

